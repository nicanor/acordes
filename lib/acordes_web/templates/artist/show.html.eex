<h1><%= @artist.name %></h1>

<article class="tab-list">
<%= for x <- 1..5 do %>
<%= for tab <- @tabs do %>
  <%= link to: "#{@artist.slug}/#{tab.slug}" do %>
    <%= tab.title %> <span class="star-ranking r5"></span>
  <% end %>
<% end %>
<% end %>
</article>

<hr />

<h2>Bio:</h2>
<source><%= @artist.bio_source %></source><br />

<%= @artist.bio %>
